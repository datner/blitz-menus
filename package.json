    "build": "next build",
    "start": "next start",
    "studio": "blitz prisma studio",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "upgrade:prisma": "yarn add prisma @prisma/client",
